	"tokenColors": [
		{
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"string.comment"
			],
			"settings": {
				"foreground": "#5c6370", // Softer comment text
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"constant",
				"entity.name.constant",
				"variable.other.constant",
				"variable.language"
			],
			"settings": {
				"foreground": "#d19a66" // Orange for constants
			}
		},
		{
			"scope": [
				"entity",
				"entity.name",
				"entity.name.type",
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#e5c07b" // Yellow for entities
			}
		},
		{
			"scope": "keyword",
			"settings": {
				"foreground": "#c678dd", // Magenta for keywords
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"storage",
				"storage.type"
			],
			"settings": {
				"foreground": "#c678dd" // Magenta for storage types
			}
		},
		{
			"scope": [
				"string",
				"punctuation.definition.string",
				"string punctuation.section.embedded source"
			],
			"settings": {
				"foreground": "#98c379" // Green for strings
			}
		},
		{
			"scope": "support",
			"settings": {
				"foreground": "#56b6c2" // Cyan for support elements
			}
		},
		{
			"scope": [
				"variable",
				"variable.parameter"
			],
			"settings": {
				"foreground": "#e06c75" // Red for variables
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#e06c75",
				"fontStyle": "underline" // Red underline for invalid elements
			}
		}
	]
}